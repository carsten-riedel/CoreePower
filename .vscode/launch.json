{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "System remove CoreePower Modules",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}\\.vscode\\Runnertest.ps1",
            "args": ["-Mode", "remove"],
            "cwd": "${workspaceFolder}",
            "createTemporaryIntegratedConsole": true
        },
        {
            "name": "PowerShell: debug.ps1 PSM1",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}\\.vscode\\Runnertest.ps1",
            "args": ["-Mode", "psm"],
            "cwd": "${workspaceFolder}",
            "createTemporaryIntegratedConsole": true
        },
        {
            "name": "PowerShell: debug.ps1 PSD1",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}\\.vscode\\Runnertest.ps1",
            "args": ["-Mode", "psd"],
            "cwd": "${workspaceFolder}",
            "createTemporaryIntegratedConsole": true
        }
    ]
}

